    "eslint-plugin-astro": "^0.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier-doc": "^1.1.0",
    "execa": "^6.1.0",
    "husky": "^8.0.1",
    "node-fetch": "^3.2.5",
